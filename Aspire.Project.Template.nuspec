      <!-- Add any dependencies if applicable -->
    </dependencies>
  </metadata>
  <files>
    <file src="src/**/*" target="content/src" />
    <file src="test/**/*" target="content/test" />
    <file src="__ProjectName__.sln" target="content" />
    <file src=".template.config/*" target="content/.template.config" />
  </files>
</package>
